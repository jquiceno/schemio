{
  "name": "schemio",
  "version": "0.2.5",
  "description": "Nodejs library to validate data schema",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test ava -v -s",
    "lint": "standard",
    "dev:test": "npm run test -- -w",
    "cover": "nyc npm run test"
  },
  "keywords": [
    "schema",
    "data",
    "validate"
  ],
  "author": "jquiceno",
  "license": "ISC",
  "ava": {
    "files": [
      "./test/*.test.js"
    ],
    "sources": [
      "**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "@hapi/boom": "^7.4.2",
    "@hapi/joi": "^15.0.3",
    "defaults": "^1.0.3"
  },
  "devDependencies": {
    "ava": "^1.2.1",
    "moment": "^2.24.0",
    "nyc": "^14.1.1",
    "standard": "^12.0.1"
  }
}
